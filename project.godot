config_version=4

_global_script_classes=[ {
"base": "CollisionShape2D",
"class": "Projectile",
"language": "GDScript",
"path": "res://src/projectile/Projectile.gd"
}, {
"base": "Projectile",
"class": "ProjectileLinear",
"language": "GDScript",
"path": "res://src/projectile/deprecated/ProjectileLinear.gd"
} ]
